{
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "rewrites": [
    {
      "source": "/game/bini-puzzle/:path*",
      "destination": "/game/bini-puzzle/:path*"
    },
    {
      "source": "/game/flappy-plane/:path*",
      "destination": "/game/flappy-plane/:path*"
    },
    {
      "source": "/game/typo-trap/:path*",
      "destination": "/game/typo-trap/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
